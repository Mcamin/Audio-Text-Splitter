<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="130.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.audiotext.split.FXMLController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="108.0" prefWidth="321.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" style="&#10;" text="Add AudioText File:" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField fx:id="filepath" layoutX="14.0" layoutY="50.0" prefHeight="25.0" prefWidth="265.0" />
            <Button fx:id="chooseButton" layoutX="294.0" layoutY="50.0" mnemonicParsing="false" onAction="#handlechooseButton" prefHeight="25.0" prefWidth="87.0" text="Open File" />
            <Button fx:id="cancelButton" layoutX="293.0" layoutY="96.0" mnemonicParsing="false" onAction="#handlecancelButton" prefHeight="25.0" prefWidth="87.0" text="Cancel" />
            <Button fx:id="saveButton" layoutX="202.0" layoutY="96.0" mnemonicParsing="false" onAction="#handleSaveButton" prefHeight="25.0" prefWidth="87.0" text="Ok" />
      </children>
    </AnchorPane>
  </children>
</VBox>
